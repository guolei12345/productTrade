#searchinput{
  width: 70%;
  height: 30px;
  margin: 2px 5px;
  padding: 0 2px;
  border: 1px solid;
  border-radius: 10px; 
}
.mySearch{
  width: 20%;
  height: 30px;
  background-color: green;
  border-radius: 10px;
}

image{
  width: 100%;
  height: 100%;
}

.weui-grid{
  width: 48%;
  background:rgb(231, 221, 203);
  padding: 5px;
  margin-right: 1%;
  margin-bottom: 1px;
}
.weui-grid__icon{
  width: 100%;
  height: 165px;
}
.weui-grid__label {
  text-align: left;
  padding-left: 1px;
}
.weui-grid .weui-grid__label:nth-child(3){
  color: red;
}

